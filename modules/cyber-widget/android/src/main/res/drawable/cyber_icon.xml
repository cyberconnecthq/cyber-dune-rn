<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#E0FFE0" /> <!-- Outer Light Green -->
        </shape>
    </item>
    <item android:left="2dp" android:right="2dp" android:top="2dp" android:bottom="2dp">
        <shape android:shape="oval">
            <solid android:color="#00FF00" /> <!-- Middle Green -->
        </shape>
    </item>
    <item android:left="4dp" android:right="4dp" android:top="4dp" android:bottom="4dp">
        <shape android:shape="oval">
            <solid android:color="#008000" /> <!-- Inner Dark Green -->
        </shape>
    </item>
    <item android:left="6dp" android:right="6dp" android:top="6dp" android:bottom="6dp">
        <shape android:shape="oval">
            <solid android:color="#F0FFF0" /> <!-- Innermost White -->
        </shape>
    </item>
</layer-list>
